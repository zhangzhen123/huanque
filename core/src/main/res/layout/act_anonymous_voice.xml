<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view_bottom_circle"
        android:layout_width="275dp"
        android:layout_height="275dp"
        android:layout_marginTop="104dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/bg_bottom_circle_anonymous"/>

    <View
        android:id="@+id/view_big_circle"
        android:layout_width="0px"
        android:layout_height="0px"
        app:layout_constraintLeft_toLeftOf="@id/view_bottom_circle"
        app:layout_constraintRight_toRightOf="@id/view_bottom_circle"
        app:layout_constraintTop_toTopOf="@id/view_bottom_circle"
        app:layout_constraintBottom_toBottomOf="@id/view_bottom_circle"/>

</androidx.constraintlayout.widget.ConstraintLayout>
